## Python 환경 및 로그 자동화 Makefile

pseudo:
	python src/deidentifier/pseudonymize_files.py

depseudo:
	python src/deidentifier/depseudonymize_files.py

valid:
	python src/deidentifier/validate_depseudonymization.py

excel:
	python src/deidentifier/excel_deidentifier.py data/raw/pet/ data/raw/pet/deidentification.yml data/deidentified/pet/

patho:
	python -m src.deidentifier.pathology_deidentifier

meta:
	python -m src.deidentifier.preliminary_pathology_metafier
	
